{
	"name": "mc-studio",
	"private": true,
	"version": "0.2.0",
	"description": "A tool to help you with your developing tasks for Minecraft",
	"author": {
		"name": "Max de Groot",
		"email": "max@maximumfx.nl",
		"url": "https://maximumfx.nl/"
	},
	"engines": {
		"node": ">=v16.13",
		"npm": ">=8.1"
	},
	"main": "packages/main/dist/index.cjs",
	"scripts": {
		"build": "npm run build:main && npm run build:preload && npm run build:renderer",
		"build:main": "cd ./packages/main && vite build",
		"build:preload": "cd ./packages/preload && vite build",
		"build:renderer": "cd ./packages/renderer && vite build",
		"compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
		"test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
		"test:e2e": "vitest run",
		"test:main": "vitest run -r packages/main --passWithNoTests",
		"test:preload": "vitest run -r packages/preload --passWithNoTests",
		"test:renderer": "vitest run -r packages/renderer --passWithNoTests",
		"watch": "node scripts/watch.js",
		"lint": "eslint . --ext js,ts,vue",
		"typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
		"typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
		"typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
		"typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
		"postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 npx --no-install electron ./scripts/update-electron-vendors.js "
	},
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^6.1.1",
		"electron-updater": "5.0.1",
		"material-symbols": "^0.2.1",
		"panzoom": "^9.4.3",
		"pinia": "^2.0.14",
		"prismjs": "^1.28.0",
		"three": "^0.130.1",
		"three-instanced-group-mesh": "^1.0.2",
		"twgl.js": "^4.23.1",
		"vue": "3.2.37",
		"vue-router": "^4.0.16"
	},
	"devDependencies": {
		"@types/dateformat": "^5.0.0",
		"@types/mime": "^2.0.3",
		"@types/pngjs": "^6.0.1",
		"@types/prismjs": "^1.26.0",
		"@types/three": "^0.142.0",
		"@types/unzipper": "^0.10.5",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "5.28.0",
		"@vitejs/plugin-vue": "3.0.0-alpha.1",
		"@vue/test-utils": "2.0.1",
		"autoprefixer": "^10.4.7",
		"cross-env": "7.0.3",
		"dateformat": "^5.0.3",
		"electron": "19.0.4",
		"electron-builder": "23.0.3",
		"electron-devtools-installer": "3.2.0",
		"eslint": "8.17.0",
		"eslint-plugin-vue": "9.1.1",
		"fast-average-color-node": "^2.4.0",
		"fast-folder-size": "^1.7.0",
		"filesize": "^9.0.9",
		"happy-dom": "5.3.1",
		"luaparse": "^0.3.1",
		"mime": "^3.0.0",
		"minecraft-data": "^3.5.0",
		"nano-staged": "0.8.0",
		"playwright": "1.22.2",
		"pngjs": "^6.0.0",
		"postcss": "^8.4.14",
		"prismarine-nbt": "^2.2.1",
		"simple-git-hooks": "2.8.0",
		"tailwindcss": "^3.1.3",
		"typescript": "4.7.3",
		"unplugin-auto-expose": "0.0.1",
		"unzipper": "^0.10.11",
		"uuid": "^8.3.2",
		"vite": "3.0.0-alpha.11",
		"vitest": "0.15.1",
		"vue-tsc": "0.37.8"
	}
}
